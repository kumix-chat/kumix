{
  "productName": "kumix",
  "version": "0.0.1",
  "identifier": "chat.kumix.desktop",
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": ["main-capability"]
    },
    "windows": [
      {
        "label": "main",
        "title": "kumix",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ]
  },
  "bundle": {},
  "plugins": {}
}
